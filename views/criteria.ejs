</ul>


    <div class="table-data" style="margin-left:25%;">
        <h2>Criteria</h2>
            <table border="1">
                <tr>
                    <th>S.N</th>
                    <th>Student Name</th>
                    <th>Department Name</th>
                    <th>Course Name</th>
                    <th>Eligibility</th>
                   
                    <th>Delete</th>
                </tr>
                
                <%
                if(userData.length!=0){
                var i=1;
                userData.forEach(function(data){
                %>
                <tr>
                    <td><%=i; %></td>
                    <td><%=data.sname %></td>
                    <td><%=data.dname %></td>
                    <td><%=data.cname %></td>
                    <td><%=data.eligibility %></td>
                
                    <td>   <button onclick="myJavascriptFunction(data.id)" type="button">Delete</button></td>
                    <td><a href="/criteria/edit/<%=data.sname%>">Delete</a></td>
                </tr>
                <%  i++; }) %>
                <% } else{ %>
                    <tr>
                        <td colspan="7">No Data Found</td>
                    </tr>
                <% } %>
            </table>
            </div>
</body>

</html>
