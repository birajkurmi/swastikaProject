    <div class="table-data"style="margin-left:25%;overflow-x:auto;">
        <h2>Courses</h2>
            <table border="1">
                <tr>
                    <th>S.N</th>
                    <th>Department Name</th>
                    <th>Course Name</th>
                    <th>Duration</th>
                    <th>fees</th>
                    
                    <th>Delete</th>
                </tr>
                
                <%
                if(userData.length!=0){
                var i=1;
                userData.forEach(function(data){
                %>
                <tr>
                    <td><%=i; %></td>
                    <td><%=data.dpt %></td>
                    <td><%=data.name %></td>
                    <td><%=data.duration %></td>
                    <td><%=data.fees %></td>
                
                    <td><a href="/course/edit/<%=data.name%>"><button onclick="myJavascriptFunction(data.id)" type="button">Delete</a></button></td>
                    <!-- <td><a href="/course/edit/<%=data.name%>"><button onclick="myJavascriptFunction(data.id)" type="button">Update</a></button></td>
                --></tr> 
                <%  i++; }) %>
                <% } else{ %>
                    <tr>
                        <td colspan="7">No Data Found</td>
                    </tr>
                <% } %>
            </table>
            </div>
</body>

